class Cake {
    taste() {
        var adjective = "delicious";
        print "The " + this.flavour + " cake is " + adjective + "!";
    }
}

var chocolateCake = Cake();
chocolateCake.flavour = "chocolate";
var chocTaste = chocolateCake.taste;

var lemonCake = Cake();
lemonCake.flavour = "lemon";
lemonCake.taste = chocTaste;
lemonCake.taste();